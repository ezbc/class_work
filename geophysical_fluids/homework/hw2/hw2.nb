Cell[CellGroupData[{

Cell["1a)", "Subsubsection",
 CellChangeTimes->{{3.653648195717279*^9, 3.653648197363305*^9}}],

Cell[TextData[{
 "The pressure difference will be given by Bernoulli\[CloseCurlyQuote]s EQ:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["P", "1"], "+", 
     SuperscriptBox[
      SubscriptBox["\[Rho]u", "1"], "2"], "+", 
     SubscriptBox["g\[Rho]h", "1"]}], " ", "=", 
    RowBox[{
     SubscriptBox["P", "2"], "+", 
     SuperscriptBox[
      SubscriptBox["\[Rho]u", "2"], "2"], "+", 
     SubscriptBox["g\[Rho]h", "2"]}]}], TraditionalForm]]],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["P", "2"], "-", 
     SubscriptBox["P", "1"]}], "=", 
    RowBox[{"\[Rho]", "[", 
     RowBox[{
      FractionBox[
       SuperscriptBox[
        SubscriptBox["u", "2"], "2"], "2"], "+", 
      SubscriptBox["gh", "2"]}], "]"}]}], TraditionalForm]]],
 "\nwhere\n",
 Cell[BoxData[{
  FormBox[
   RowBox[{"\[Rho]", "=", 
    RowBox[{
     SubscriptBox["\[Rho]", "air"], "=", 
     RowBox[{"1.2", " ", 
      RowBox[{"kg", " ", "/", " ", 
       SuperscriptBox["m", "3"]}]}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"h", "=", 
    RowBox[{"50", " ", "m"}]}], TraditionalForm]}]],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"g", "=", 
    RowBox[{"10", " ", 
     RowBox[{"m", "/", 
      SuperscriptBox["s", "2"]}]}]}], TraditionalForm]]],
 "\[LineSeparator]",
 Cell[BoxData[
  FormBox[
   RowBox[{"u", "=", 
    RowBox[{"10", 
     RowBox[{"m", "/", "s"}]}]}], TraditionalForm]]],
 "\nthen\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["P", "'"], "=", 
    RowBox[{
     RowBox[{"660", " ", 
      FractionBox["kg", 
       RowBox[{"m", " ", 
        SuperscriptBox["s", "2"]}]]}], "=", 
     RowBox[{"660", "Pa"}]}]}], TraditionalForm]]]
}], "Text",
 CellChangeTimes->{{3.653648204686296*^9, 3.6536482499272738`*^9}, {
  3.653648300519691*^9, 3.653648309703185*^9}, {3.653648419658514*^9, 
  3.65364842186197*^9}, {3.654354976234565*^9, 3.654355079915572*^9}, {
  3.6543552324460506`*^9, 3.654355388779152*^9}}]
}, Open  ]],

Cell[CellGroupData[{

Cell["1b)", "Subsubsection",
 CellChangeTimes->{{3.6543554183060293`*^9, 3.654355419144434*^9}}],

Cell[TextData[{
 "The pressure difference between upstream of the flow and at the top of the \
building will be\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["P", "'"], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["P", "2"], "-", 
      SubscriptBox["P", "1"]}], "=", 
     RowBox[{
      FractionBox["\[Rho]", "2"], "[", 
      RowBox[{
       SuperscriptBox[
        SubscriptBox["u", "1"], "2"], "-", 
       SuperscriptBox[
        SubscriptBox["u", "2"], "2"]}], "]"}]}]}], TraditionalForm]]],
 "\nwe estimate ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["u", "1"], "=", " ", 
    RowBox[{
     RowBox[{
      SubscriptBox["u", "2"], "+", 
      RowBox[{"1", 
       RowBox[{"m", "/", "s"}]}]}], "=", 
     RowBox[{"9", " ", 
      RowBox[{"m", "/", "s"}]}]}]}], TraditionalForm]]],
 ", then\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["P", "'"], "=", 
    RowBox[{
     FractionBox["1.2", "2"], 
     RowBox[{"kg", "/", 
      RowBox[{
       SuperscriptBox["m", "3"], "(", 
       RowBox[{
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"9", 
           RowBox[{"m", "/", "s"}]}], ")"}], "2"], "-", 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"10", 
           RowBox[{"m", "/", "s"}]}], ")"}], "2"]}], ")"}]}]}]}], 
   TraditionalForm]]],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["P", "'"], "=", 
    RowBox[{
     RowBox[{"-", "11"}], " ", "Pa"}]}], TraditionalForm]]]
}], "Text",
 CellChangeTimes->{{3.654355471312523*^9, 3.6543554758403587`*^9}, {
   3.654355648411569*^9, 3.6543556782478237`*^9}, {3.654355864578512*^9, 
   3.65435586797222*^9}, {3.6543560577536793`*^9, 3.654356097905176*^9}, {
   3.654356260744084*^9, 3.654356292673427*^9}, {3.654356336331243*^9, 
   3.654356369863607*^9}, {3.654356451998047*^9, 3.654356545879339*^9}, {
   3.654356836169468*^9, 3.6543568403150578`*^9}, {3.6543568919762583`*^9, 
   3.654356916249009*^9}, 3.65496839271454*^9, 3.654968548209133*^9, {
   3.654968876492858*^9, 3.6549689127002363`*^9}, {3.6549689507823553`*^9, 
   3.654968956721939*^9}, {3.654969084838028*^9, 3.654969086839511*^9}}]
}, Open  ]],

Cell[CellGroupData[{

Cell["1c)", "Subsubsection",
 CellChangeTimes->{{3.653652426172208*^9, 3.653652427682081*^9}, {
  3.65365310288238*^9, 3.653653103172165*^9}}],

Cell[TextData[{
 "We predict the normalized pressure in the pipe to be\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox[
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["P", "1"], "-", 
        SubscriptBox["P", "2"]}], ")"}], 
      SuperscriptBox["d", "3"], "\[Rho]"}], 
     RowBox[{
      SuperscriptBox["\[Nu]", "2"], 
      SuperscriptBox["\[Rho]", "2"], "L"}]], "=", 
    RowBox[{
     FractionBox[
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["P", "1"], "-", 
         SubscriptBox["P", "2"]}], ")"}], 
       SuperscriptBox["d", "3"]}], 
      RowBox[{
       SuperscriptBox["\[Nu]", "2"], "\[Rho]", " ", "L"}]], "=", 
     RowBox[{
      FractionBox[
       RowBox[{
        RowBox[{"(", 
         RowBox[{"660", " ", 
          FractionBox["kg", 
           RowBox[{"m", " ", 
            SuperscriptBox["s", "2"]}]]}], ")"}], " ", "*", 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"0.1", " ", "m"}], ")"}], "3"]}], 
       RowBox[{
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{
           SuperscriptBox["10", 
            RowBox[{"-", "5"}]], 
           FractionBox[
            SuperscriptBox["m", "2"], "s"]}], ")"}], 
         RowBox[{"2", "\[LineSeparator]"}]], "*", "1.2", " ", 
        FractionBox["kg", 
         SuperscriptBox["m", "3"]], "*", "100", "m"}]], "=", 
      RowBox[{"5.5", " ", 
       SuperscriptBox["10", "7"]}]}]}]}], TraditionalForm]]],
 "\nwhich is beyond the scope of the measured pressures in the diagram. We \
estimate the logarithmic slope of the relationship with finite differencing \
then  can solve for Re\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"Re", "=", 
    RowBox[{
     SuperscriptBox["10", 
      FractionBox[
       RowBox[{
        RowBox[{"log", "(", 
         RowBox[{"5.5", " ", 
          SuperscriptBox["10", "7"]}], ")"}], "-", "1.5"}], "1.7"]], "=", 
     RowBox[{"4.7", " ", 
      SuperscriptBox["10", "4"]}]}]}], TraditionalForm]]],
 "\n\nwith an estimated Re we are able to determine the flow speed:\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"Re", "=", 
    FractionBox["uL", "\[Nu]"]}], TraditionalForm]]],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"u", "=", 
    RowBox[{
     FractionBox[
      RowBox[{"Re", " ", "\[Nu]"}], "L"], "=", 
     FractionBox[
      RowBox[{"4.6", " ", 
       SuperscriptBox["10", "4"], "*", "2", " ", 
       SuperscriptBox["10", 
        RowBox[{"-", "5"}]], 
       RowBox[{
        SuperscriptBox["m", "2"], "/", "s"}]}], 
      RowBox[{"100", " ", "m"}]]}]}], TraditionalForm]]],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"u", "=", 
    RowBox[{"9", " ", 
     SuperscriptBox["10", 
      RowBox[{"-", "3"}]], 
     RowBox[{"m", "/", "s"}]}]}], TraditionalForm]]]
}], "Text",
 CellChangeTimes->{{3.65365243098265*^9, 3.653652617756534*^9}, {
  3.653652674302335*^9, 3.653652725920429*^9}, {3.6536527715844183`*^9, 
  3.653652793279405*^9}, {3.653652843239605*^9, 3.653652846095744*^9}, {
  3.653652908464798*^9, 3.653652965562113*^9}, {3.653653019182173*^9, 
  3.653653099769558*^9}, {3.653653191323367*^9, 3.653653248954967*^9}, {
  3.6536532849458647`*^9, 3.653653476089347*^9}, {3.6536535414324102`*^9, 
  3.653653555196575*^9}, {3.653653669209405*^9, 3.6536537179763927`*^9}, {
  3.653653869848413*^9, 3.653653895115877*^9}, {3.6536539392798023`*^9, 
  3.653653982908996*^9}, {3.653654084276227*^9, 3.653654191594195*^9}, {
  3.65365431634203*^9, 3.653654440304906*^9}, {3.6549598657814407`*^9, 
  3.654959867357609*^9}, {3.6551975191328287`*^9, 3.655197630736912*^9}}]
}, Open  ]],

Cell[CellGroupData[{

Cell["2a)", "Subsubsection",
 CellChangeTimes->{{3.653653575105791*^9, 3.653653576127778*^9}, {
  3.6536547496768017`*^9, 3.65365475052297*^9}}],

Cell[TextData[{
 "The viscous shear is given by\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Tau]", "=", 
    RowBox[{
     FractionBox[
      RowBox[{"\[PartialD]", "v"}], 
      RowBox[{"\[PartialD]", "r"}]], "~", 
     FractionBox["v", "r"]}]}], TraditionalForm]]],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"v", "=", 
    RowBox[{"\[Tau]", " ", "*", " ", "2", "\[Pi]", " ", "500", " ", "km"}]}], 
   TraditionalForm]]],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"v", "~", "300"}], " ", 
    RowBox[{"m", "/", "s"}]}], TraditionalForm]]],
 "\n\n",
 StyleBox["2b)", "Subsubsection"]
}], "Text",
 CellChangeTimes->{{3.6536547533206*^9, 3.653654903647272*^9}, {
  3.653654954360057*^9, 3.653654960603253*^9}, {3.65365537693006*^9, 
  3.653655456962851*^9}, {3.654447925727723*^9, 3.654447926518696*^9}, {
  3.655197635191835*^9, 3.655197651919887*^9}}],

Cell[TextData[{
 "The vorticity is ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Omega]", "=", 
    RowBox[{
     RowBox[{"\[Del]", "\[Cross]", "v"}], "=", 
     RowBox[{
      FractionBox["1", "r"], 
      FractionBox[
       RowBox[{"\[PartialD]", 
        RowBox[{"(", 
         RowBox[{"r", " ", "v"}], ")"}]}], 
       RowBox[{"\[PartialD]", "r"}]]}]}]}], TraditionalForm]]],
 " in the k direction given the symmetry of the problem in cyclindrical \
coordinates.\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Phi]", "=", 
    RowBox[{
     RowBox[{
      RowBox[{"1", "/", "r"}], " ", "*", " ", 
      FractionBox[
       RowBox[{"\[PartialD]", "rv"}], 
       RowBox[{"\[PartialD]", "r"}]]}], "=", 
     RowBox[{
      FractionBox["v", "r"], "+", 
      FractionBox[
       RowBox[{"\[PartialD]", "v"}], 
       RowBox[{"\[PartialD]", "r"}]]}]}]}], TraditionalForm]]],
 "\nwhere ",
 Cell[BoxData[
  FormBox[
   FractionBox["v", "r"], TraditionalForm]]],
 " is the curvature vorticity equal to the angular frequency \[CapitalOmega] \
and ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"\[PartialD]", "v"}], 
    RowBox[{"\[PartialD]", "r"}]], TraditionalForm]]],
 " is the shear vorticity. Assuming v does not vary hugely over r,\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Phi]", " ", "=", 
    RowBox[{
     FractionBox[
      RowBox[{"2", "v"}], "r"], "=", 
     RowBox[{
      RowBox[{"2", "\[CapitalOmega]"}], "=", 
      RowBox[{
       FractionBox[
        RowBox[{"2", " ", "300", " ", 
         RowBox[{"m", "/", "s"}]}], 
        RowBox[{"5", " ", 
         SuperscriptBox["10", "5"], "m"}]], "=", 
       RowBox[{"1.2", " ", 
        SuperscriptBox["10", 
         RowBox[{"-", "3"}]], 
        SuperscriptBox["s", 
         RowBox[{"-", "1"}]]}]}]}]}]}], TraditionalForm]]],
 "\n\n",
 StyleBox["2c)", "Subsubsection"]
}], "Text",
 CellChangeTimes->{{3.653655685310047*^9, 3.653655958310071*^9}, {
   3.6543574018865557`*^9, 3.654357404147744*^9}, {3.654357478307633*^9, 
   3.6543574873640203`*^9}, {3.654357523253295*^9, 3.654357604267674*^9}, {
   3.654357653762801*^9, 3.65435771296865*^9}, {3.654357765651368*^9, 
   3.6543578027864313`*^9}, {3.654447920019539*^9, 3.654447923114073*^9}, 
   3.654958949818885*^9}],

Cell[TextData[{
 "The angular frequency is\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[CapitalOmega]", "=", 
    FractionBox["v", "r"]}], TraditionalForm]]],
 "\n\nthus\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Phi]", "=", 
    RowBox[{
     RowBox[{"2", "\[CapitalOmega]"}], "=", 
     RowBox[{"1.2", " ", 
      SuperscriptBox["10", 
       RowBox[{"-", "3"}]], 
      SuperscriptBox["s", 
       RowBox[{"-", "1"}]]}]}]}], TraditionalForm]]],
 "\n"
}], "Text",
 CellChangeTimes->{{3.6536559404000607`*^9, 3.653655940786365*^9}, {
   3.653656017624803*^9, 3.653656020344475*^9}, 3.653656572251059*^9, {
   3.654358301198473*^9, 3.654358347653788*^9}, {3.6551976648117027`*^9, 
   3.655197687965907*^9}}]
}, Open  ]],

Cell[CellGroupData[{

Cell["3a)", "Subsubsection",
 CellChangeTimes->{{3.654358355305131*^9, 3.654358355877743*^9}, {
  3.655197686007227*^9, 3.6551976866591682`*^9}}],

Cell[TextData[{
 "We can estimate the scale height by calculating the difference in heights \
using the energy conservation from sea level to the desired height\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["h", "1"], "=", 
    FractionBox[
     RowBox[{
      RowBox[{
       SubscriptBox["c", "V"], " ", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          SubscriptBox["\[Rho]", "0"], 
          SubscriptBox["T", "0"]}], "-", 
         RowBox[{
          SubscriptBox["\[Rho]", "1"], 
          SubscriptBox["T", "1"]}]}], ")"}]}], " ", "+", " ", 
      SubscriptBox["P", "0"], "-", 
      SubscriptBox["P", "1"]}], 
     RowBox[{
      RowBox[{
       SubscriptBox["\[Rho]", "1"], "g"}], " ", "+", " ", 
      RowBox[{
       SubscriptBox["c", "V"], 
       SubscriptBox["\[Rho]", "1"], 
       SubscriptBox["m", "T"]}]}]]}], TraditionalForm]]],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["h", "1"], "=", 
    FractionBox[
     RowBox[{
      SubscriptBox["P", "0"], "+", 
      SubscriptBox["P", "1"]}], 
     RowBox[{
      RowBox[{
       SubscriptBox["\[Rho]", "1"], "g"}], " ", "+", " ", 
      RowBox[{
       SubscriptBox["c", "V"], 
       SubscriptBox["\[Rho]", "1"], 
       SubscriptBox["m", "T"]}]}]]}], TraditionalForm]]],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["h", "1"], "=", 
    FractionBox[
     RowBox[{
      RowBox[{"101325", " ", "Pa"}], " ", "-", " ", 
      RowBox[{"50000", "Pa"}]}], 
     RowBox[{
      RowBox[{"1.2", " ", 
       FractionBox["kg", 
        SuperscriptBox["m", "3"]], " ", "9.8", 
       FractionBox["m", 
        SuperscriptBox["s", "2"]]}], "+", 
      RowBox[{"700", " ", 
       FractionBox[
        RowBox[{"kg", " ", 
         SuperscriptBox["m", "2"]}], 
        SuperscriptBox["s", "2"]], " ", "*", "1.2", 
       FractionBox["kg", 
        SuperscriptBox["m", "3"]], "*", "6500", 
       FractionBox["K", "m"]}]}]]}], TraditionalForm]]],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["h", "2"], "=", 
    FractionBox[
     RowBox[{
      RowBox[{"101325", " ", "Pa"}], " ", "-", " ", 
      RowBox[{"100000", "Pa"}]}], 
     RowBox[{
      RowBox[{"1.2", " ", 
       FractionBox["kg", 
        SuperscriptBox["m", "3"]], " ", "9.8", 
       FractionBox["m", 
        SuperscriptBox["s", "2"]]}], "+", 
      RowBox[{"700", " ", 
       FractionBox[
        RowBox[{"kg", " ", 
         SuperscriptBox["m", "2"]}], 
        SuperscriptBox["s", "2"]], " ", "*", "1.2", 
       FractionBox["kg", 
        SuperscriptBox["m", "3"]], "*", "6500", 
       FractionBox["K", "m"]}]}]]}], TraditionalForm]]],
 "\n\nLeading to a scale height of \n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["h", "2"], "-", 
     SubscriptBox["h", "1"]}], "=", 
    RowBox[{"2.9", " ", "km"}]}], TraditionalForm]]],
 "\n\nAnalyzing\n\
http://weather.unisys.com/upper_air/ua_cont.php?plot=thi&inv=0&t=cur\nwe find \
the thickness over WI is 5 km. This agrees mildly with our estimated scale \
height."
}], "Text",
 CellChangeTimes->{{3.654450511115562*^9, 3.6544506300015163`*^9}, {
  3.6544506808321667`*^9, 3.654450772524723*^9}, {3.654450805937347*^9, 
  3.654451092249098*^9}, {3.65445171036452*^9, 3.654451718903599*^9}, {
  3.6544519200739098`*^9, 3.65445194464929*^9}, {3.6544522592489443`*^9, 
  3.654452274582481*^9}, {3.6551976955318613`*^9, 3.6551977841641073`*^9}}]
}, Open  ]],

Cell[CellGroupData[{

Cell["4a)", "Subsubsection",
 CellChangeTimes->{{3.6544530420386543`*^9, 3.6544530425695953`*^9}}],

Cell[TextData[{
 "We estimate the 4km geostropic wind speed over Tateno Japan with\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["u", "g"], "=", 
    RowBox[{
     RowBox[{"-", 
      FractionBox["1", "\[Rho]f"]}], 
     FractionBox[
      RowBox[{"\[PartialD]", "P"}], 
      RowBox[{"\[PartialD]", "y"}]]}]}], TraditionalForm]]],
 "\n\nWe estimate ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[CapitalDelta]P", "=", 
    RowBox[{"500", "Pa"}]}], TraditionalForm]]],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[CapitalDelta]y", "=", 
    RowBox[{"5", " ", 
     SuperscriptBox["10", "5"], "m"}]}], TraditionalForm]]],
 " using finite differencing, leading us to an estimate of ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["u", "g"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[CapitalOmega]", "=", 
    RowBox[{
     SuperscriptBox["10", 
      RowBox[{"-", "5"}]], "Hz"}]}], TraditionalForm]]],
 "\n",
 Cell[BoxData[
  FormBox[
   SubscriptBox["u", "g"], TraditionalForm]]],
 "=-",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox["1", 
     RowBox[{"1.2", " ", "*", 
      FractionBox["kg", 
       SuperscriptBox["m", "3"]], " ", "2", " ", "*", " ", 
      SuperscriptBox["10", 
       RowBox[{"-", "5"}]], 
      SuperscriptBox["s", 
       RowBox[{"-", "1"}]], "*", 
      RowBox[{"Sin", "[", "0.69", "]"}]}]], "*", 
    SuperscriptBox["10", 
     RowBox[{"-", "3"}]], 
    FractionBox[
     FractionBox[
      FractionBox[
       RowBox[{"kg", " ", 
        SuperscriptBox["m", "2"]}], 
       SuperscriptBox["s", "2"]], 
      SuperscriptBox["m", "3"]], "m"]}], TraditionalForm]]],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["u", "g"], "=", 
    RowBox[{
     RowBox[{"-", "65"}], " ", 
     RowBox[{"m", "/", "s"}]}]}], TraditionalForm]]],
 "\n\nCompared to the measured observations in winter (20 m/s) we are within \
approximation errors."
}], "Text",
 CellChangeTimes->{{3.65445304657205*^9, 3.654453180594117*^9}, {
   3.654453210836274*^9, 3.65445322138428*^9}, {3.654453437117044*^9, 
   3.654453522824469*^9}, 3.6551977953319473`*^9, {3.655197907163431*^9, 
   3.655198009853669*^9}}]
