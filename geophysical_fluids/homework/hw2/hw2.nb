Cell[CellGroupData[{

Cell["4b)", "Subsubsection",
 CellChangeTimes->{{3.6544532440234118`*^9, 3.654453246762013*^9}}],

Cell[TextData[{
 "We can estimate the wind speed by relating the vertical acceleration to the \
temperature gradient\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox[
     RowBox[{"\[PartialD]", "u"}], 
     RowBox[{"\[PartialD]", "z"}]], "=", 
    RowBox[{
     FractionBox["g", 
      RowBox[{"f", " ", "T"}]], 
     FractionBox[
      RowBox[{"\[PartialD]", "T"}], 
      RowBox[{"\[PartialD]", "x"}]]}]}], TraditionalForm]]],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"u", "=", 
    RowBox[{
     FractionBox["g", 
      RowBox[{"f", " ", "T"}]], 
     FractionBox[
      RowBox[{"\[PartialD]", "T"}], 
      RowBox[{"\[PartialD]", "x"}]], "z"}]}], TraditionalForm]]],
 "\n\nwhere ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox[
     RowBox[{"\[PartialD]", "T"}], 
     RowBox[{"\[PartialD]", "x"}]], "=", 
    RowBox[{
     RowBox[{"lapse", " ", "rate"}], "=", 
     RowBox[{
      RowBox[{
       FractionBox["1", "100"], 
       FractionBox["K", "km"]}], "=", 
      RowBox[{
       SuperscriptBox["10", 
        RowBox[{"-", "5"}]], 
       FractionBox["K", "m"]}]}]}]}], TraditionalForm]]],
 ", and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"z", "=", 
    RowBox[{
     RowBox[{"5", " ", "km"}], "=", 
     RowBox[{"5", " ", 
      SuperscriptBox["10", "3"], "m"}]}]}], TraditionalForm]]],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{"f", "=", 
    RowBox[{"2", " ", "*", " ", 
     SuperscriptBox["10", 
      RowBox[{"-", "5"}]], 
     SuperscriptBox["s", 
      RowBox[{"-", "1"}]], "*", 
     RowBox[{"Sin", "[", "0.75", "]"}]}]}], TraditionalForm]]],
 ",\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"u", "=", 
    RowBox[{
     FractionBox[
      RowBox[{"9.8", " ", 
       RowBox[{"m", "/", 
        SuperscriptBox["s", "2"]}]}], 
      RowBox[{"2", " ", "*", " ", 
       SuperscriptBox["10", 
        RowBox[{"-", "5"}]], 
       SuperscriptBox["s", 
        RowBox[{"-", "1"}]], "*", 
       RowBox[{"Sin", "[", "0.75", "]"}], " ", "290", " ", "K"}]], 
     SuperscriptBox["10", 
      RowBox[{"-", "5"}]], 
     FractionBox["K", "m"], " ", "5", " ", 
     SuperscriptBox["10", "3"], "m"}]}], TraditionalForm]]],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"u", "=", 
    RowBox[{"123", 
     RowBox[{"m", "/", "s"}]}]}], TraditionalForm]]],
 "\n"
}], "Text",
 CellChangeTimes->{
