   3.654605968057988*^9, 3.654606254798842*^9}, {3.654606285895453*^9, 
   3.654606556367323*^9}, {3.654606836426507*^9, 3.654607071893433*^9}, {
   3.654607136933208*^9, 3.654607581434712*^9}, {3.6546076255158157`*^9, 
   3.654607625515849*^9}, {3.654607715469027*^9, 3.6546077483788013`*^9}, {
   3.6546077820552397`*^9, 3.6546078008132763`*^9}, {3.6546078485235357`*^9, 
   3.654607855839919*^9}, {3.654607924761326*^9, 3.654607930137885*^9}, {
