Cell[CellGroupData[{

Cell["5a)", "Subsubsection",
 CellChangeTimes->{{3.654603587718972*^9, 3.654603591257955*^9}}],

Cell[TextData[{
 "The coriolis and curvature terms in the horizontal momentum equations are\n\
\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     OverscriptBox["i", "^"], " ", ":", " ", 
     FractionBox["du", "dt"]}], "=", 
    RowBox[{
     FractionBox[
      RowBox[{"2", "u", " ", "v", " ", 
       RowBox[{"tan", "[", "\[Phi]", "]"}]}], "z"], "+", 
     RowBox[{"2", " ", "\[CapitalOmega]", " ", "v", " ", 
      RowBox[{"sin", "[", "\[Phi]", "]"}], " "}]}]}], TraditionalForm]]],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     OverscriptBox["j", "^"], " ", ":", " ", 
     FractionBox["dv", "dt"]}], "=", 
    RowBox[{
     RowBox[{"-", 
      FractionBox[
       RowBox[{
        SuperscriptBox["u", "2"], " ", 
        RowBox[{"tan", "[", "\[Phi]", "]"}]}], "z"]}], "-", 
     RowBox[{"2", " ", "\[CapitalOmega]", " ", "u", " ", 
      RowBox[{"sin", "[", "\[Phi]", "]"}]}]}]}], TraditionalForm]]],
 " \n\ngiven the following known quantities\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Phi]", "=", 
    RowBox[{"43.0667", " ", "deg"}]}], TraditionalForm]]],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"d", "=", 
    RowBox[{
     RowBox[{"100", "km"}], "=", 
     RowBox[{
      SuperscriptBox["10", "5"], "m"}]}]}], TraditionalForm]]],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"z", "=", 
    RowBox[{"6", " ", 
     SuperscriptBox["10", "6"], "m"}]}], TraditionalForm]]],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"u", "=", 
    RowBox[{
     RowBox[{"-", 
      FractionBox["1000", 
       SqrtBox["2"]]}], 
     FractionBox["m", "s"]}]}], TraditionalForm]]],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"v", "=", 
    RowBox[{"-", "u"}]}], TraditionalForm]]],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[CapitalOmega]", "=", 
    RowBox[{"7", " ", 
     SuperscriptBox["10", 
      RowBox[{"-", "5"}]], "Hz"}]}], TraditionalForm]]],
 "\n\nwe can calculate the horizontal accelerations\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox["du", "dt"], "=", 
    RowBox[{
     RowBox[{
      FractionBox[
       RowBox[{"2", "*", 
        FractionBox[
         RowBox[{"-", "1000"}], 
         SuperscriptBox["2", "0.5"]], 
        FractionBox["m", "s"], "*", 
        FractionBox["1000", 
         SuperscriptBox["2", "0.5"]], 
        FractionBox["m", "s"]}], 
       RowBox[{"6.371", " ", 
        SuperscriptBox["10", "6"], "m"}]], 
      RowBox[{"Tan", "[", 
       RowBox[{"43.0667", " ", "deg", " ", "*", " ", 
        FractionBox["\[Pi]", 
         RowBox[{"180", " ", "deg"}]]}], "]"}]}], "+", 
     RowBox[{"2", "*", "7", " ", 
      SuperscriptBox["10", 
       RowBox[{"-", "5"}]], 
      RowBox[{"1", "/", "s"}], "*", 
      FractionBox["1000", 
       SuperscriptBox["2", "0.5"]], 
      FractionBox["m", "s"], "*", 
      RowBox[{"Sin", "[", 
       RowBox[{"43.0667", " ", "deg", " ", "*", " ", 
        FractionBox["\[Pi]", 
         RowBox[{"180", " ", "deg"}]]}], "]"}]}]}]}], TraditionalForm]]],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox["dv", "dt"], "=", 
    RowBox[{
     RowBox[{
      FractionBox[
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{
          FractionBox["1000", 
           SuperscriptBox["2", "0.5"]], 
          FractionBox["m", "s"]}], ")"}], "2"], 
       RowBox[{"6.371", " ", 
        SuperscriptBox["10", "6"], "m"}]], 
      RowBox[{"Tan", "[", 
       RowBox[{"43.0667", " ", "deg", " ", "*", " ", 
        FractionBox["\[Pi]", 
         RowBox[{"180", " ", "deg"}]]}], "]"}]}], "-", 
     RowBox[{"2", "*", "7", " ", 
      SuperscriptBox["10", 
       RowBox[{"-", "5"}]], 
      RowBox[{"1", "/", "s"}], "*", 
      FractionBox[
       RowBox[{"-", "1000"}], 
       SuperscriptBox["2", "0.5"]], 
      FractionBox["m", "s"], "*", 
      RowBox[{"Sin", "[", 
       RowBox[{"43.0667", " ", "deg", " ", "*", " ", 
        FractionBox["\[Pi]", 
         RowBox[{"180", " ", "deg"}]]}], "]"}]}]}]}], TraditionalForm]]],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox["du", "dt"], "=", 
    RowBox[{"-", 
     FractionBox[
      RowBox[{"0.079112073722698`", " ", "m"}], 
      SuperscriptBox["s", "2"]]}]}], TraditionalForm]]],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox["dv", "dt"], "=", 
    FractionBox[
     RowBox[{"0.14095398347613258`", " ", "m"}], 
     SuperscriptBox["s", "2"]]}], TraditionalForm]]],
 "\n\nTo calculate the time traveled we use the displacement EQ\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", " ", "=", " ", 
    RowBox[{
     RowBox[{"u", " ", "t"}], " ", "+", " ", 
     RowBox[{"a", " ", 
      SuperscriptBox["t", "2"]}]}]}], TraditionalForm]]],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["y", "'"], "=", 
    RowBox[{
     SuperscriptBox["x", "'"], "=", 
     FractionBox["d", 
      SqrtBox["2"]]}]}], TraditionalForm]]],
 "\n\nThus the time without deflection is\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"t", "=", 
    RowBox[{
     FractionBox[
      SuperscriptBox["x", "'"], "u"], "=", 
     RowBox[{
      FractionBox["d", "u"], "=", 
      RowBox[{
       SuperscriptBox["10", "2"], "s"}]}]}]}], TraditionalForm]]],
 "\n\nWith the curvature terms, we need to account for acceleration in the \
displacement of the missile:\n\n\[CapitalDelta]",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", " ", "=", 
    RowBox[{
     RowBox[{
      SuperscriptBox["x", "'"], "-", 
      RowBox[{"(", " ", 
       RowBox[{
        RowBox[{"u", " ", "t"}], " ", "+", " ", 
        RowBox[{
         FractionBox["du", "dt"], " ", 
         SuperscriptBox["t", "2"]}]}], ")"}]}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{"-", 
        FractionBox[
         SuperscriptBox["10", "5"], 
         SuperscriptBox["2", "0.5"]]}], "m"}], "-", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         RowBox[{"-", 
          FractionBox["1000", 
           SuperscriptBox["2", 
            RowBox[{"1", "/", "2"}]]]}], 
         FractionBox["m", "s"], "*", 
         SuperscriptBox["10", "2"], "s"}], "-", 
        RowBox[{
         FractionBox[
          RowBox[{"0.07911", " ", "m"}], 
          SuperscriptBox["s", "2"]], "*", 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{
            SuperscriptBox["10", "2"], "s"}], ")"}], "2"]}]}], ")"}]}]}]}], 
   TraditionalForm]]],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[CapitalDelta]y", " ", "=", " ", 
    RowBox[{
     RowBox[{
      SuperscriptBox["y", "'"], "-", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"v", " ", "t"}], " ", "+", " ", 
        RowBox[{
         FractionBox["dv", "dt"], " ", 
         SuperscriptBox["t", "2"]}]}], ")"}]}], "=", 
     RowBox[{
      RowBox[{
       FractionBox[
        SuperscriptBox["10", "5"], 
        SuperscriptBox["2", "0.5"]], "m"}], "-", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         FractionBox["1000", 
          SuperscriptBox["2", 
           RowBox[{"1", "/", "2"}]]], 
         FractionBox["m", "s"], "*", 
         SuperscriptBox["10", "2"], "s"}], "+", 
        RowBox[{
         FractionBox[
          RowBox[{"0.1409", " ", "m"}], 
          SuperscriptBox["s", "2"]], "*", 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{
            SuperscriptBox["10", "2"], "s"}], ")"}], "2"]}]}], ")"}]}]}]}], 
   TraditionalForm]]],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[CapitalDelta]x", "=", 
    RowBox[{"800", "m"}]}], TraditionalForm]]],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[CapitalDelta]y", "=", 
    RowBox[{
     RowBox[{"-", "1400"}], " ", "m"}]}], TraditionalForm]]]
}], "Text",
 CellChangeTimes->{{3.6546038619356403`*^9, 3.654603862730259*^9}, 
   3.654603960712846*^9, {3.654604004524651*^9, 3.6546040828153954`*^9}, {
   3.65460411837015*^9, 3.6546042931376343`*^9}, {3.654604332746244*^9, 
   3.654604365147657*^9}, {3.654604552369461*^9, 3.654604552683895*^9}, {
   3.654605720414237*^9, 3.6546057235049753`*^9}, {3.654605773082925*^9, 
